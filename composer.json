{
    "name": "thiagobueno/zoop-sdk",
    "description": "SDK de pagamentos Zoop",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "zoop",
        "payment",
        "gateway "
    ],
    "authors": [
        {
            "name": "Thiago Bueno",
            "email": "thiago@nerdetcetera.com"
        }
    ],
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/log": "^1.1",
        "guzzlehttp/guzzle": ">=6.2",
        "italodeveloper/guzzle-zend": "^1.2"
    },
    "require-dev": {
        "zendframework/zend-http": ">=2.5"
    },
    "autoload": {
        "psr-4": {
            "Zoop\\": "src/"
        }
    }
}
